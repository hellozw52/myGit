<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ssh.pojo">
	<class name="ShopCategory" table="shopcategory">
		<id name="shopcategoryid" type="int">
		    <column name="shopcategoryid"/>
			<generator class="increment" />
		</id>

		<property name="shopcategory" length="10"></property>
		<property name="pid" length="10"></property>
	</class>
</hibernate-mapping>
